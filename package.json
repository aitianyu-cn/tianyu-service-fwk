{
    "name": "@aitianyu.cn/tianyu-service-fwk",
    "version": "0.0.1",
    "description": "Tianyu Framework for backend server",
    "main": "./dist/lib/index.js",
    "types": "./dist/types/index.d.ts",
    "exports": {
        ".": "./dist/lib/index.js"
    },
    "files": [
        "dist"
    ],
    "keywords": [
        "aitianyu.cn",
        "server",
        "service",
        "framework"
    ],
    "type": "commonjs",
    "scripts": {
        "build": "npm run before-build && tsc && npm run copy-lib && npm run copy-types && npm run after-build",
        "copy-lib": "copyfiles -u 1 build/**/*.js dist/lib",
        "copy-types": "copyfiles -u 1 build/**/*.d.ts dist/types",
        "before-build": "rimraf ./dist ./build",
        "after-build": "rimraf ./build",
        "test": "jest --no-cache --verbose",
        "test-all": "jest --no-cache --verbose --coverage",
        "test-coverage": "jest --no-cache --coverage"
    },
    "author": "dev@aitianyu.cn",
    "license": "ISC",
    "devDependencies": {
        "@types/jest": "^29.2.4",
        "@types/node": "^18.11.15",
        "copyfiles": "^2.4.1",
        "jest": "^29.3.1",
        "jest-html-reporters": "^3.0.11",
        "rimraf": "^3.0.2",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "tsc-alias": "^1.8.2",
        "typescript": "^4.9.3"
    },
    "dependencies": {
        "@aitianyu.cn/types": "latest",
        "fs": "^0.0.1-security",
        "path": "^0.12.7"
    }
}
